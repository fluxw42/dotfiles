!Xft.autohint: true
!Xft.antialias: true
!Xft.hinting: true
!Xft.hintstyle: hintslight
!Xft.dpi: 96
!Xft.rgba: rgb
!Xft.lcdfilter: lcddefault


URxvt*scrollBar: false

URxvt*background: #000000
URxvt*foreground: #FFFFFF

URxvt*color8: #000000
URxvt*color9: #CD0000
URxvt*color10: #00CD00
URxvt*color11: #CDCD00
